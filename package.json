{
  "name": "ynotsoft-dynamic-grid",
  "version": "1.0.25",
  "description": "DynamicGrid Component",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && cp dist/grid.css grid.css",
    "preview": "vite preview"
  },
  "main": "dist/dynamic-grid.umd.cjs",
  "module": "dist/dynamic-grid.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ynotsoft/dynamic-grid"
  },
  "exports": {
    ".": {
      "import": "./dist/dynamic-grid.js",
      "require": "./dist/dynamic-grid.umd.cjs"
    },
    "./grid.css": "./dist/grid.css"
  },
  "files": [
    "dist",
    "grid.css",
    "README.md"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "@tailwindcss/vite": "^4.1.0",
    "tailwindcss": "^4.1.0"
  },
  "dependencies": {
    "lucide-react": "^0.553.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "vite": "^7.2.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "react",
    "components",
    "ui"
  ],
  "license": "MIT"
}
